  "scripts": {
    "test:prepare": "node_modules/.bin/cross-env NODE_ENV=test node_modules/.bin/mocha --compilers js:babel-register --require shared/test-helper.js --bail",
    "test:server": "npm run test:prepare -- server/**/*.spec.js",
    "test:server:watch": "npm run test:server -- --watch",
    "test:client": "npm run test:prepare -- client/**/*.spec.js",
    "test:client:watch": "npm run test:client -- --watch",
    "test:shared": "npm run test:prepare -- shared/**/*.spec.js",
    "test:shared:watch": "shared/**/*.spec.js",
    "test:all": "npm run test:prepare -- server/**/*.spec.js client/**/*.spec.js shared/**/*.spec.js",
    "test:all:watch": "watch-run -i -p './shared/**/*.jsx?,./shared/**/*.jsx?' npm run test:all",
    "tape:prepare": "node_modules/.bin/cross-env NODE_ENV=test node_modules/.bin/tape-watch ./shared/**/*.spec.js -r babel-register ./shared/test-helper.js | faucet",
    "tape:shared": "npm run tape:prepare -- shared/**/*.spec.js",
    "start": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/nodemon index.js",
    "start:prod": "node_modules/.bin/cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run start:prod",
    "minify": "cleancss -o static/css/app.min.css static/css/app.css",
    "build": "node_modules/.bin/cross-env NODE_ENV=production webpack --config webpack.config.prod.js && npm run minify",
    "clean": "rimraf static/dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "node_modules/.bin/eslint client server shared",
    "watch": "npm-watch"
  },
  "watch": {
    "test:shared:watch": "{shared}/*.spec.js"
  },
  "pre-commit": [
    "lint",
    "test",
    "test:server"
  ],
